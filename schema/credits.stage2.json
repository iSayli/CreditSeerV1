{
    "stage": "stage2",
    "sectionType": "credits",
    "schemasByValueType": {
        "fee": {
            "feeRate": {
                "extractionHint": {
                "pattern": "(\\d+\\.\\d+%|\\d+%|Applicable Fee|Applicable Margin)",
                "notes": "Extract the fee rate. It may be a percentage or a defined term like 'Applicable Fee' or 'Applicable Margin'.",
                "type": "text"
                }
            },
            "feeBase": {
                "extractionHint": {
                "pattern": "(average daily unused amount|LC Exposure|Revolving Commitment)",
                "notes": "Extract the base on which the fee is calculated.",
                "type": "text"
                }
            },
            "accrualPeriod": {
                "extractionHint": {
                "pattern": "(from and including|from and after|during the period|commencing on|beginning on).*?(to but excluding|until|through)",
                "notes": "Extract the accrual period language.",
                "type": "text"
                }
            },
            "paymentTiming": {
                "collectMultiple": true,
                "extractionHint": {
                "pattern": "(payable in arrears|payable on|on the Closing Date|on demand)",
                "notes": "Extract when the fee is payable.",
                "type": "text"
                }
            },
            "exclusions": {
                "extractionHint": {
                "pattern": "Defaulting Lender",
                "notes": "Extract any exclusions (e.g., Defaulting Lenders).",
                "type": "text"
                }
            }
        },
        "interest": {
            "baseRate": {
                "extractionHint": {
                "pattern": "(Alternate Base Rate|Adjusted LIBO Rate|SOFR Rate|Prime Rate)",
                "notes": "Extract the base rate used. The pattern given is a list of possible base rates.",
                "type": "text"
                }
            },
            "marginReference": {
                "extractionHint": {
                "pattern": "Applicable Margin",
                "notes": "Extract reference to Applicable Margin.",
                "type": "text"
                }
            },
            "interestRateFormula": {
                "collectMultiple": true,
                "outputMode": "summarized",
                "extractionHint": {
                "pattern": "(shall bear interest|rate per annum)",
                "notes": "Summarize how interest is calculated using only the block text.",
                "type": "text"
                }
            },
            "paymentTiming": {
                "extractionHint": {
                "pattern": "(Interest Payment Dates|payable in arrears|payable on demand)",
                "notes": "Extract when interest is payable.",
                "type": "text"
                }
            }
        }

    }
}